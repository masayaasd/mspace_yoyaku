{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/admin/(.*)",
            "destination": "/admin/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}